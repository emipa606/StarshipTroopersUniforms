<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <!-- ==================================== Trooper SST =====================================-->
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_SSTtrooper</defName>
    <label>Trooper Uniform (SST)</label>
    <description>The standard, grey uniform used by Mobile Infantrymen, these uniforms give no better protection than the cloth it is made of. Bullets will rip through any ape in this outfit. It looks great though!</description>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Apparel/Display/Trooper_Uniform</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <costList>
      <Cloth>110</Cloth>
    </costList>
    <thingCategories>
      <!--<li>Apparel</li>-->
      <li>StarshipTroopers_Clothing</li>
    </thingCategories>
    <statBases>
      <Mass>0.50</Mass>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToMake>12000</WorkToMake>
      <ArmorRating_Blunt>0.08</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.08</ArmorRating_Sharp>
      <Insulation_Cold>10</Insulation_Cold>
    </statBases>
    <equippedStatOffsets>
      <WorkSpeedGlobal>0.15</WorkSpeedGlobal>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Waist</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Apparel/Clothing/trooper</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
      <tags>
        <li>SST_TrooperUniform</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
